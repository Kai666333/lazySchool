<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_home">
    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.lazysch.ui.takeRequires.TakeRequiresFragment"
        android:label="@string/title_home"
        android:layout="@layout/fragment_take_requires" >
    </fragment>
    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.lazysch.ui.driverDashboard.DriverDashboardFragment"
        android:label="@string/title_dashboard"
        android:layout="@layout/fragment_driverdashboard" />
    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.lazysch.ui.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />

    <fragment
        android:id="@+id/navigation_personal"
        android:name="com.example.lazysch.ui.personal.PersonalFragment"
        android:label="@string/title_personal"
        tools:layout="@layout/fragment_personal" />


</navigation>